  <head>
    <title>Contact Me</title>
    <link rel="stylesheet" href="assets/css/contact.css">
    <link rel="icon" href="assets/images/34464790.png">
  </head>
  
  <nav>
    <div class="nav-left">
      <img src="https://dumbways.id/assets/images/brandred.png" alt="logo-DW" width="10%" />
      <ul>
        <li>
          <a href="/">Home</a>
        </li>
        <li>
          <a href="/myproject">My Project</a>
        </li>
      </ul>
    </div>
    <div class="nav-right">
      <a href="/contact">Contact Me</a>
    </div>
  </nav>

  <body>
    <form action="#">
      <div>
        <label for="input-name">Name</label>
        <input id="input-name" type="text" name="inputName">
      </div>
      <div>
        <label for="input-email">Email</label>
        <input id="input-email" type="email" name="inputEmail">
      </div>
      <div>
        <label for="input-number">Phone Number</label>
        <input id="input-number" type="number" name="inputNumber">
      </div>
      <div>
        <label for="input-subject">Subject</label>
        <select id="input-subject">
            <option value="">Select</option>
            <option value="Frontend" name="Frontend">Frontend</option>
            <option value="Backend" name="Backend">Backend</option>
            <option value="Fullstack" name="Fullstack">Fullstack</option>
        </select>
      </div>
      <div>
        <label for="input-message">Message</label>
        <textarea name="" id="input-message" cols="30" rows="10" style="resize: none" name="inputMessage"></textarea>
      </div>
      <div>
        <button type="button" onclick="submitData()" >Submit</button>
      </div>
    </form>
    <script src="assets/js/form.js" ></script>
  </body>
</html>